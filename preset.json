{
  "useConfigFiles": true,
  "plugins": {
    "@vue/cli-plugin-babel": {},
    "@vue/cli-plugin-typescript": {
      "classComponent": false,
      "useTsWithBabel": true
    },
    "@vue/cli-plugin-router": {
      "historyMode": true
    },
    "@vue/cli-plugin-vuex": {},
    "@vue/cli-plugin-eslint": {
      "config": "airbnb",
      "lintOn": [
        "save",
        "commit"
      ]
    },
    "vue-cli-plugin-vuetify": {
		  "version": "~2.4.0",
		  "preset": "configure",
		  "replaceComponents": true,
		  "iconFont": "mdi",
		  "installFonts": true,
		  "locale": "ru",
		  "useAlaCarte": true,
		  "useCustomProperties": false,
		  "usePolyfill": true,
		  "useTheme": false,
		  "useV3": false,
		  "useVite": false
		}
  },
  "vueVersion": "2",
  "cssPreprocessor": "dart-sass"
}
